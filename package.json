{
  "name": "@qonsoll/theme-generator",
  "version": "1.2.0",
  "description": "Generate theme for your next project",
  "main": "build/index.js",
  "module": "build/index.esm.js",
  "files": [
    "build"
  ],
  "private": false,
  "scripts": {
    "build": "rollup -c",
    "test": "jest",
    "test:watch": "jest --watch",
    "format": "prettier --write src/**/*.ts{,x}",
    "lint": "tsc --noEmit && eslint src/**/*.ts{,x}"
  },
  "keywords": [
    "palette",
    "colors",
    "theme",
    "scheme",
    "brand"
  ],
  "author": "Oleksiy Pastukhov",
  "license": "MIT",
  "devDependencies": {
    "@babel/core": "^7.12.13",
    "@rollup/plugin-babel": "^5.2.3",
    "@rollup/plugin-commonjs": "^17.1.0",
    "@rollup/plugin-json": "^4.1.0",
    "@rollup/plugin-node-resolve": "^11.1.1",
    "rollup": "^2.38.5",
    "rollup-plugin-terser": "^7.0.2"
  },
  "eslintConfig": {
    "extends": "./eslintrc.js"
  },
  "dependencies": {
    "chroma-js": "^2.1.0",
    "color": "^3.1.3"
  }
}
